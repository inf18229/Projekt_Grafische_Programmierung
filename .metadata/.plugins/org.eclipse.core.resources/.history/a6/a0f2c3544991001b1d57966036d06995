package myCar;
import resources.rad;

class ausweichen {
	@generated("blockdiagram", "f8c07b0a")
	public rad steering_out(boolean in emerg_msg) {
		if (emerg_msg) {
			return 0.21[rad]; // Main/steering_out 1/if-then 1
		} else {
			return 0.0[rad]; // Main/steering_out 1/if-else 1
		} // Main/steering_out 1
	}
}